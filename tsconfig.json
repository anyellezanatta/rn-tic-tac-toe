{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/storybook/*": [".storybook/*"],
      "@/*": ["src/*"]
    }
  },
  "include": ["index.ts", "src/**/*", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
